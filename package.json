{
  "name": "litterarum-api",
  "version": "0.0.1",
  "description": "Litterarum's API",
  "main": "dist/server.js",
  "scripts": {
    "dev": "ts-node-dev --files -r tsconfig-paths/register ./src/server.ts",
    "build": "tsc && tsc-alias",
    "start": "tsc && tsc-alias && node .",
    "test": "cross-env NODE_ENV=test jest --coverage --detectOpenHandles --testTimeout=10000",
    "format": "prettier --write {**/,}*.js",
    "format:check": "prettier --check {**/,}*.js",
    "lint": "eslint ."
  },
  "author": "Royer Gomez",
  "license": "ISC",
  "dependencies": {
    "ajv": "^8.11.2",
    "cookie-parser": "^1.4.4",
    "cors": "^2.8.4",
    "dotenv": "^16.0.3",
    "express": "^4.17.1",
    "helmet": "^6.0.1",
    "hpp": "^0.2.3",
    "lodash": "^4.17.13",
    "mongodb": "^3.0.7",
    "mongoose": "^6.0.9",
    "morgan": "^1.10.0",
    "passport": "^0.6.0",
    "passport-http-bearer": "^1.0.1",
    "webdav": "^4.11.2",
    "winston": "^3.8.2"
  },
  "devDependencies": {
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/hpp": "^0.2.2",
    "@types/jest": "^29.5.0",
    "@types/morgan": "^1.9.3",
    "@types/passport": "^1.0.11",
    "@types/passport-http-bearer": "^1.0.37",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^5.46.0",
    "@typescript-eslint/parser": "^5.46.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.29.0",
    "eslint-config-prettier": "^8.5.0",
    "jest": "^29.5.0",
    "prettier": "2.8.1",
    "supertest": "^6.3.3",
    "ts-jest": "^29.0.5",
    "ts-node-dev": "^2.0.0",
    "tsc-alias": "^1.8.2",
    "tsconfig-paths": "^4.1.1",
    "typescript": "^4.9.4"
  }
}
