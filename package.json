{
  "name": "litterarum-api",
  "version": "0.0.1",
  "description": "Litterarum's API",
  "main": "dist/src/server.js",
  "scripts": {
    "dev": "ts-node-dev --exit-child --files -r tsconfig-paths/register ./src/server.ts",
    "clean": "rimraf dist",
    "compile": "tsc && tsc-alias ",
    "copy-files": "copyfiles public/* dist",
    "build": "npm run clean && npm run compile && npm run copy-files",
    "start": "node .",
    "test": "cross-env NODE_ENV=test jest --coverage --testTimeout=10000",
    "format": "prettier --write {**/,}*.js",
    "format:check": "prettier --check {**/,}*.js",
    "lint": "eslint .",
    "prepare": "husky"
  },
  "author": "Royer Gomez",
  "license": "ISC",
  "dependencies": {
    "ajv": "^8.11.2",
    "cookie-parser": "^1.4.4",
    "copyfiles": "^2.4.1",
    "cors": "^2.8.4",
    "dotenv": "^16.0.3",
    "express": "^4.17.1",
    "helmet": "^6.0.1",
    "hpp": "^0.2.3",
    "lodash": "^4.17.13",
    "mongodb": "^3.0.7",
    "mongoose": "^7.6.0",
    "morgan": "^1.10.0",
    "passport": "^0.6.0",
    "passport-http": "^0.3.0",
    "rimraf": "^5.0.1",
    "webdav": "^4.11.2",
    "winston": "^3.8.2"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.2.0",
    "@commitlint/config-conventional": "^19.1.0",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/hpp": "^0.2.2",
    "@types/jest": "^29.5.0",
    "@types/morgan": "^1.9.3",
    "@types/passport": "^1.0.11",
    "@types/passport-http": "^0.3.9",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^5.46.0",
    "@typescript-eslint/parser": "^5.46.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.29.0",
    "eslint-config-prettier": "^8.5.0",
    "express-rate-limit": "^6.7.0",
    "husky": "^9.0.11",
    "jest": "^29.5.0",
    "prettier": "2.8.1",
    "supertest": "^6.3.3",
    "ts-jest": "^29.0.5",
    "ts-node-dev": "^2.0.0",
    "tsc-alias": "^1.8.2",
    "tsconfig-paths": "^4.1.1",
    "typescript": "^4.9.4"
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  }
}
