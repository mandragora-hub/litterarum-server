{
  "name": "litterarum-api",
  "version": "0.0.1",
  "description": "Litterarum's API",
  "main": "dist/server.js",
  "scripts": {
    "dev": "node scripts/dev-watch.js",
    "watch:ts": "tsc --watch",
    "watch:ts-alias": "tsc-alias --watch",
    "watch:node": "node --watch .",
    "prebuild": "eslint .",
    "build": "tsc && tsc-alias",
    "prestart": "npm run build",
    "start": "node .",
    "test": "echo \"Error: no test specified\" && exit 1",
    "format": "prettier --write {**/,}*.js",
    "lint": "prettier --check {**/,}*.js"
  },
  "author": "Royer Gomez",
  "license": "ISC",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "cookie-parser": "^1.4.4",
    "cors": "^2.8.4",
    "dotenv": "^16.0.3",
    "express": "^4.17.1",
    "lodash": "^4.17.13",
    "mongodb": "^3.0.7",
    "mongoose": "^6.0.9",
    "simple-node-logger": "^21.8.12",
    "validator": "^13.7.0"
  },
  "devDependencies": {
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.14",
    "@typescript-eslint/eslint-plugin": "^5.46.0",
    "@typescript-eslint/parser": "^5.46.0",
    "concurrently": "^7.6.0",
    "eslint": "^8.29.0",
    "prettier": "^2.0.5",
    "tsc-alias": "^1.8.2",
    "typescript": "^4.9.4"
  }
}